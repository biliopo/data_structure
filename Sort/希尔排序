#include <stdio.h>
int n;

void ShellInsert(int k[],int d){
    int i,j,m,temp;
    for(i=d;i<n;i++){
        if(k[i]<k[i-d]){
            temp=k[i];
            for(j=i-d;j>=0&&temp<k[j];j-=d){
                k[j+d]=k[j];
            }
            k[j+d]=temp;
        }
    }
    for(m=0;m<n;m++){
        printf("%d ",k[m]);
    }
}

main(){
    int j;
    scanf("%d",&n);
    int d,k[n];
    for(j=0;j<n;j++){
        scanf("%d",&k[j]);
    }
    for(d=n/2;d>0;d/=2){
        ShellInsert(k,d);
        printf("\n");
    }
    return 0;
}
