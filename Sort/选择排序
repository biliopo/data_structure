#include <stdio.h>

main(){
    int n;
    scanf("%d",&n);
    int k[n];
    for(int i=0;i<n;i++){
        scanf("%d",&k[i]);
    }
    int min;
    int flag;
    for(int j=0;j<n-1;j++){
            min=k[j];
            flag=j;
        for(int m=j+1;m<n;m++){
            if(min>k[m]){
                min=k[m];
                flag=m;
            }
        }
        if(flag!=j){
            k[flag]=k[j];
            k[j]=min;
        }
        for(int a=0;a<n;a++){
            printf("%d ",k[a]);
        }
        printf("\n");
    }
}
