#include<stdio.h>
#include<stdlib.h>

main(){
    int i,j,temp;
    int n,m;
    scanf("%d",&n);
    int k[n],l[n];
    for(m=0;m<n;m++){
        scanf("%d",&k[m]);
    }
    for(i=0;i<n-1;i++){
        int flag=0;
        for(j=0;j<n-1-i;j++){
            if(k[j]>k[j+1]){
                temp=k[j];
                k[j]=k[j+1];
                k[j+1]=temp;
            }
            }
            //对排序结果进行判断，若两次排序结果一样，则排序已完成
            if(i==0){
                for(m=0;m<n;m++)
                printf("%d ",l[m]=k[m]);
            }
            else{
                for(m=0;m<n;m++){
                    if(l[m]==k[m]){
                        flag++;
                    }
                    else break;
                }
                if(flag==n){
                    for(m=0;m<n;m++)
                printf("%d ",l[m]=k[m]);
                    break;
                }
                else {
                    for(m=0;m<n;m++){
                printf("%d ",l[m]=k[m]);
                }
            }
    }
    if(flag==n) break;
    printf("\n");
        }
return 0;
}
